<h1 style="padding-left:5%;transform:translateY(-8%)">thirdpage</h1>
<form action="/fourthpage" method="POST">

<input type="hidden" placeholder="first name" name="first" value="<%=first%>" required>
                     <input type="hidden" placeholder="last name" name="last" value="<%=last%>" required>

                     <input type="hidden" placeholder="AccountType" name="profiletype" value="<%=profileType%>" required>

                     <input type="hidden" placeholder="Phone" name="phone"  value="<%=phone%>" required>

                          <input type="hidden"  name="gender" value="<%=gender%>" required>


<div class="form-group">
<input type="hidden" placeholder="D-O-B" name="dob"  value="<%=dob%>" class="form-control" style="width:270px">
</div>


<div class="form-group">
<input type="hidden" placeholder="Religion" name="religion"  value="<%=religion%>" class="form-control" style="width:270px">
</div>

<div class="form-group">
<input type="hidden" placeholder="Mother-Tongue" name="mt"  value="<%=mt%>" class="form-control" style="width:270px">
