<div class="form-group">
<input type="hidden" placeholder="email" name="email" value="<%=email%>" class="form-control" style="width:270px">
</div>


<div class="form-group">
<input type="hidden" placeholder="Password" name="password"  value="<%=password%>" class="form-control" style="width:270px">
</div>


<div class="form-group">
<input type="hidden" placeholder="Caste" name="caste"  value="<%=caste%>" class="form-control" style="width:270px">
</div>



<div class="form-group">
<input type="hidden"   value="<%=willing%>" name="willing">
</div>


<div class="form-group">
<input type="text" placeholder="height" name="height"   class="form-control" style="width:270px">
</div>

<div class="form-group">
<input type="text" placeholder="Marital Status" name="ms"  class="form-control" style="width:270px">
</div>


<div class="form-group">
<input type="checkbox"   value="yes" name="disab">Any Disability?
</div>



<div class="form-group">
<input type="text" placeholder="Family Status" name="fs"  class="form-control" style="width:270px">
</div>

<div class="form-group">
<input type="text" placeholder="Family Type" name="ft" class="form-control" style="width:270px">
</div>

<div class="form-group">
<input type="text" placeholder="Family Value" name="fv" class="form-control" style="width:270px">
</div>

<button class="btn btn-lg btn-primary btn-block" style="width:270px">Done</button>
</form>
</div>
